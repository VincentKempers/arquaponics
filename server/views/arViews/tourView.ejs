<%- include('../partials/header.ejs') %>

	<a-scene
		embedded
		arjs='debugUIEnabled: true;'
		inspector="url: https://aframe.io/releases/0.3.0/aframe-inspector.min.js"
	>
		<!-- Defined marker content -->
		<a-marker id="a-greenhouse" preset='hiro'>
			<!-- here define the content to display on top of the marker -->
			<a-text value="TEST" rotation="270 0 0" position="0 0 0"></a-text>
			
			
			<%# <a-entity rotation="270 0 0" line="start: 1 2 3; end: 4 5 6; color: #ff0"></a-entity> %>
			<%#
			<a-entity
				rotation="270 0 0"
				geometry="primitive: plane; width: 4; height: auto"
				material="color: blue"
				text="value: This text will be 4 units wide.">
			</a-entity>
			%>


			<a-curve id="track1" type="CatmullRom">
				<%#
				<a-curve-point position="-2 1 -3"></a-curve-point>
				<a-curve-point position="0 0 -3"></a-curve-point>
				<a-curve-point position="2 1 -5"></a-curve-point>
				%>
			</a-curve>

			<a-draw-curve curveref="#track1" material="shader: line; color: blue;"></a-draw-curve>

     		 <a-entity clone-along-curve="curve: #track1; spacing: 0.2; scale: 1 1 1; rotation: 0 0 0;" geometry="primitive:box; height:0.1; width:0.2; depth:0.1"></a-entity>



		</a-marker>

		<!--  -->
		<a-marker id="a-cafe" preset='kanji'>
			<a-box position='.25 .25 -.25' material='color: blue;'></a-box>
		</a-marker>

		<a-marker preset='custom' type='pattern' url='/ar/arqua.patt'>
			<a-sphere position='0 0.5 0' material='color: yellow;'></a-sphere>
		</a-marker>


		<%#
		<a-marker type='pattern' url='/ar/greenhouse-pattern.patt'>
			<a-box position='0 0.5 0' material='color: black;'></a-box>
		</a-marker>
		%>

		<a-entity camera></a-entity>
	</a-scene>




	<script src="/scripts/arGraphs.js"></script>

<%- include('../partials/footer.ejs') %>
